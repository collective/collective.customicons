<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.customicons"
    purge="False">

    <configlet
        title="Settings for the collective.customicons Iconpool"
        action_id="customicons"
        appId="collective.customicons"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@customicons-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>